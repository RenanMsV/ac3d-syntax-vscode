{
    "name": "ac3d-syntax",
    "displayName": "Inivis AC3D Syntax",
    "description": "Basic syntax highlighting and snippets for AC3D files",
    "version": "0.0.3",
    "publisher": "Renan-MsV",
    "author": {
        "name": "RenanMsV https://github.com/RenanMsV"
    },
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.0.0"
    },
    "license": "GPL",
    "bugs": {
        "url": "https://github.com/RenanMsV/ac3d-syntax-vscode/issues",
        "email": "none@none.none"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/RenanMsV/ac3d-syntax-vscode.git"
    },
    "homepage": "https://github.com/RenanMsV/ac3d-syntax-vscode",
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "keywords": [
        "ac3d",
        "flightgear",
        "x-plane",
        "3d"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ac3d",
                "aliases": ["AC3D"],
                "extensions": [".ac"],
                "configuration": "./languages/ac3d/language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ac3d",
                "scopeName": "source.ac",
                "path": "./grammars/ac3d.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "ac3d",
                "scopeName": "source.ac",
                "path": "./snippets/ac3d.json"
            }
        ]
    },
    "main": "./out/extension.js",
    "activationEvents": [
        "onLanguage:ac3d"
    ],
    "devDependencies": {
        "@types/node": "^24.0.10",
        "@types/vscode": "^1.0.0",
        "typescript": "^5.8.3",
        "vscode-languageserver": "^9.0.1",
        "vscode-languageclient": "^9.0.1",
        "vscode-languageserver-textdocument": "^1.0.12"
    },
    "dependencies": {}
}
